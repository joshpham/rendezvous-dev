<h1>Listing suggesteds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @suggesteds.each do |suggested| %>
      <tr>
        <td><%= suggested.name %></td>
        <td><%= link_to 'Show', suggested %></td>
        <td><%= link_to 'Edit', edit_suggested_path(suggested) %></td>
        <td><%= link_to 'Destroy', suggested, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Suggested', new_suggested_path %>
